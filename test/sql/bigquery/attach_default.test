# name: test/sql/bigquery/attach_defaults.test
# description: Testing DEFAULT statement
# group: [bigquery]

require bigquery

require-env BQ_TEST_PROJECT

statement ok
ATTACH 'project=${BQ_TEST_PROJECT} dataset=test_dataset' AS bq (TYPE bigquery);

statement ok
DROP TABLE IF EXISTS bq.test_dataset.defaults_table;

statement ok
CREATE TABLE bq.test_dataset.defaults_table (i INTEGER DEFAULT 5, s STRING DEFAULT 'some_default');

statement ok
