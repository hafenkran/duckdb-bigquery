# name: test/sql/storage/attach_Types_arrays.test
# description: Testing BigQuery Views
# group: [storage]

require bigquery

require-env BQ_ATTACH_DB_URL

statement ok
ATTACH 'project=test api_endpoint=http://${BQ_ATTACH_DB_URL}:9050 grpc_endpoint=${BQ_ATTACH_DB_URL}:9060' AS test (TYPE bigquery);

statement ok
DROP TABLE IF EXISTS test.dataset1.table_arrays;

statement ok
CREATE TABLE test.dataset1.table_arrays AS
SELECT
    int_array,
    varchar_array,
FROM test_all_types();
