# name: test/sql/storage/attach_clear_cache.test
# description: Clear BigQuery caches
# group: [storage]

require bigquery

require-env BQ_ATTACH_DB_URL

statement ok
ATTACH 'project=test api_endpoint=http://${BQ_ATTACH_DB_URL}:9050 grpc_endpoint=${BQ_ATTACH_DB_URL}:9060' AS test (TYPE bigquery);

statement ok
DROP TABLE IF EXISTS test.dataset1.cache_test;

statement ok
CREATE TABLE test.dataset1.cache_test(i INTEGER);

statement ok
CALL bigquery_clear_cache();

query I
INSERT INTO test.dataset1.cache_test VALUES (42);
----
1

statement ok
CALL bigquery_clear_cache();

query I
SELECT * FROM test.dataset1.cache_test;
----
42

statement ok
CALL bigquery_clear_cache();

statement error
INSERT INTO test.dataset1.tst VALUES (84)
----
Catalog Error: Table with name tst does not exist!
