# name: test/sql/storage/attach_insert_table.test
# description: Test INSERT statement on tables
# group: [storage]

require bigquery

require-env BQ_ATTACH_DB_URL

statement ok
ATTACH 'project=test api_endpoint=http://${BQ_ATTACH_DB_URL}:9050 grpc_endpoint=${BQ_ATTACH_DB_URL}:9060' AS test (TYPE bigquery);

statement ok
DROP TABLE IF EXISTS test.dataset1.test_insert;

statement ok
CREATE TABLE test.dataset1.test_insert(a INTEGER, b VARCHAR NOT NULL, c FLOAT);

query I
INSERT INTO test.dataset1.test_insert VALUES (1, 'a', 0.1), (2, 'b', 0.2), (3, 'c', 0.3);
----
3

statement ok
INSERT INTO test.dataset1.test_insert VALUES (NULL, NULL, NULL);

statement error
INSERT INTO test.dataset1.test_insert VALUES (3, 'c', 0.3, 'overflow');
----
table test_insert has 3 columns but 4 values were supplied

statement error
INSERT INTO test.dataset1.test_insert VALUES (3, 'c');
----
table test_insert has 3 columns but 2 values were supplied

statement error
INSERT INTO test.dataset1.test_insert VALUES ('not INT32', 'c', 0.3);
----
Could not convert string 'not INT32' to INT32

statement ok
INSERT INTO test.dataset1.test_insert VALUES (5, NULL, 0.3);
